		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"svelte": "^4.2.7",
		"svelte-adapter-executable": "workspace:*",
		"svelte-check": "^3.6.0",
		"svelte-preprocess": "^6.0.2",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.3.3"
	},
	"type": "module"
}
